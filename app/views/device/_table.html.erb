<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">Temp</th>
      <th scope="col">Humidity</th>
      <th scope="col">Pressure</th>
      <th scope="col">Created at</th>
    </tr>
  </thead>
  <tbody>
    <% data.each do |d| %>
        <tr>
            <td><%= d.id %></td>
            <td><%= d.temp %></td>
            <td><%= d.humidity %></td>
            <td><%= d.pressure %></td>
            <td><%= d.created_at %></td>
        </tr>
    <% end %>
  </tbody>
</table>